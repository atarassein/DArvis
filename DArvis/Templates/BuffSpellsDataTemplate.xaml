<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DArvis"
                    xmlns:models="clr-namespace:DArvis.Models">

    <DataTemplate x:Key="BuffSpellsDataTemplate" DataType="{x:Type models:BuffCheckboxViewModel}">
        <CheckBox Content="{Binding Spell.Name}"
                  IsChecked="{Binding IsChecked, Mode=TwoWay}"
                  Style="{StaticResource ObsidianCheckBox}"
                  Margin="5,2" />
    </DataTemplate>

</ResourceDictionary>